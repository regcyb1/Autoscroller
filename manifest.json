{
    "manifest_version": 3,
    "name": "Auto Scroll",
    "version": "1.0",
    "description": "Automatically scrolls the page with adjustable speed.",
    "permissions": ["activeTab"],
    "action": {
      "default_popup": "popup.html"
    },
    "background": {
      "service_worker": "background.js"
    }
    
  }
  